  "scripts": {
    "npm-versions": "npm -v && node -v",
    "eslint": "node ./node_modules/eslint/bin/eslint.js .",
    "apigeelint": "node ./node_modules/apigeelint/cli.js -e PO003,PO007,TD002 -s ./apiproxy --profile apigeex -f table",
